{
  "name": "@pixelfederation/gh-action-yaml-to-json",
  "version": "0.0.4",
  "description": "yaml to json and validate",
  "repository": "https://github.com/pixelfederation/gh-action-yaml-to-json",
  "author": "Tomas Hulata <thulata@pixelfederation.com>",
  "license": "MIT",
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/main.ts --bundle --outdir=. --target=node16 --platform=node"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.1.1",
    "@actions/io": "1.1.3",
    "@kubectl/actions": "pixelfederation/gh-action-kubectl",
    "jsonpath-plus": "^7.2.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "esbuild": "0.19.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
