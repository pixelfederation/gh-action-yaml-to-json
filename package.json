{
  "name": "@pixelfederation/gh-action-yaml-to-json",
  "version": "0.0.4",
  "description": "yaml to json and validate",
  "repository": "https://github.com/pixelfederation/gh-action-yaml-to-json",
  "author": "Tomas Hulata <thulata@pixelfederation.com>",
  "license": "MIT",
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/main.ts --bundle --outdir=. --target=node20 --platform=node"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "@actions/io": "1.1.3",
    "@kubectl/actions": "pixelfederation/gh-action-kubectl",
    "jsonpath-plus": "^8.0.0",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  }
}
